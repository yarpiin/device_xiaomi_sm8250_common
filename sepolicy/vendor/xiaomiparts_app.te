# Allow xiaomiparts to read and write to vendor_sysfs_graphics
allow xiaomiparts_app vendor_sysfs_graphics:dir search;
allow xiaomiparts_app vendor_sysfs_graphics:file { read write getattr open };

allow xiaomiparts_app system_data_file:dir { search };

allow xiaomiparts_app content_capture_service:service_manager find;
allow xiaomiparts_app game_service:service_manager find;
allow xiaomiparts_app hint_service:service_manager find;
allow xiaomiparts_app netstats_service:service_manager find;
allow xiaomiparts_app thermal_service:service_manager find;
allow xiaomiparts_app trust_service:service_manager find;

