<resources>

    <!-- The padding on the top of the statusbar (usually 0). -->
    <dimen name="status_bar_padding_top">8dp</dimen>

    <!-- for 18dp of padding at 2.75px/dp at default density -->
    <dimen name="rounded_corner_content_padding">49.5px</dimen>

    <!-- The starting padding for the clock in the status bar. -->
    <dimen name="status_bar_clock_starting_padding">3dp</dimen>

    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom">30dp</dimen>

    <!--  Allow CornerHandleView and PathSpecCornerPathRenderer to decouple from corner-radius -->
    <dimen name="config_rounded_mask_size">122px</dimen>

    <!-- Location on the screen of the center of the power button -->
    <dimen name="physical_power_button_center_screen_location_y">980px</dimen>

    <!-- Location on the screen of the center of the volume up/volume down buttons -->
    <dimen name="physical_volume_up_button_center_screen_location_y">550px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">740px</dimen>

</resources>
